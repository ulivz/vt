{
  "name": "vuepress-theme-vt",
  "version": "0.4.1",
  "description": "A VuePress theme inspired by `@vue/theme`, fully compatible with VuePress default theme.",
  "keywords": [
    "documentation",
    "generator",
    "vue",
    "vuepress"
  ],
  "license": "MIT",
  "main": "index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "dev": "npm run build -- -w",
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "typescript": "4.5.3"
  },
  "dependencies": {
    "@vuepress/plugin-active-header-links": "1.9.7",
    "@vuepress/plugin-nprogress": "1.9.7",
    "@vuepress/shared-utils": "1.9.7",
    "@vuepress/types": "1.9.7",
    "cheap-watch": "1.0.3",
    "docsearch.js": "^2.5.2",
    "flexsearch": "^0.6.32",
    "html-to-text": "^7.0.0",
    "lodash": "^4.17.15",
    "markdown-it-attrs": "^2",
    "node-google-translate-skidz": "1.1.2",
    "prism-themes": "1.9.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "vue": "^2",
    "vue-router": "^3.4.5",
    "vuepress-plugin-container": "^2.0.2",
    "vuepress-plugin-smooth-scroll": "^0.0.3"
  },
  "files": [
    "components",
    "global-components",
    "layouts",
    "plugins",
    "styles",
    "lib",
    "!lib/*.tsbuildinfo",
    "!lib/*.map",
    "types",
    "*.d.ts"
  ],
  "author": "ULIVZ <chl814@foxmail.com>"
}
